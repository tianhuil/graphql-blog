{
  "name": "graphql-blog",
  "version": "0.0.2",
  "description": "A typescript blog for graphql",
  "main": "index.js",
  "scripts": {
    "docker-up": "docker-compose up",
    "docker-down": "docker-compose down",
    "prisma-deploy": "prisma deploy -e env/dev.env",
    "prisma-generate": "prisma generate -e env/dev.env",
    "prisma-seed": "prisma seed -e env/dev.env",
    "nexus-query": "ts-node --transpile-only src/script/nexus-query.ts",
    "start": "DOTENV_CONFIG_PATH=env/dev.env ts-node-dev -r dotenv/config --no-notify --respawn --transpileOnly ./src",
    "prisma": "prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianhuil/graphql-blog.git"
  },
  "author": "Tianhui Li",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tianhuil/graphql-blog/issues"
  },
  "homepage": "https://github.com/tianhuil/graphql-blog#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-yoga": "^1.18.0",
    "nexus": "^0.12.0-beta.6",
    "nexus-prisma": "^0.3.7",
    "node": "^12.6.0",
    "prisma-client-lib": "^1.34.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "apollo-boost": "^0.4.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prisma": "^1.34.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  },
  "keywords": []
}
