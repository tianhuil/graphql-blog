{
  "name": "graphql-blog",
  "version": "0.0.4",
  "description": "A typescript blog for graphql",
  "main": "index.js",
  "scripts": {
    "docker-up": "docker-compose up",
    "docker-down": "docker-compose down",
    "clear-dev": "DOTENV_CONFIG_PATH=env/dev.env ts-node -r dotenv/config --transpile-only src/script/clear.ts",
    "clear-test": "DOTENV_CONFIG_PATH=env/test.env ts-node -r dotenv/config --transpile-only src/script/clear.ts",
    "deploy-dev": "prisma deploy -e env/dev.env",
    "deploy-test": "prisma deploy -e env/test.env",
    "generate-dev": "prisma generate -e env/dev.env",
    "seed-dev": "prisma seed -e env/dev.env",
    "nexus-query": "ts-node --transpile-only src/script/nexus-query.ts",
    "nexus-deploy": "nexus-prisma-generate --client src/generated/prisma-client --output src/generated/nexus-prisma",
    "start": "DOTENV_CONFIG_PATH=env/dev.env ts-node-dev -r dotenv/config --no-notify --respawn ./src",
    "prisma": "prisma",
    "test": "DOTENV_CONFIG_PATH=env/test.env jest test",
    "fast-watch-test": "DOTENV_CONFIG_PATH=env/test.env jest test -o --watch",
    "unit-test": "jest unit.test",
    "int-test": "DOTENV_CONFIG_PATH=env/test.env jest int.test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianhuil/graphql-blog.git"
  },
  "author": "Tianhui Li",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tianhuil/graphql-blog/issues"
  },
  "homepage": "https://github.com/tianhuil/graphql-blog#readme",
  "_comment": "lodash dependency to fix severe vulnerability",
  "dependencies": {
    "bcrypt": "3.0.6",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-yoga": "^1.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": ">=4.17.13",
    "nexus": "^0.12.0-beta.6",
    "nexus-prisma": "^0.3.7",
    "node": "^12.6.0",
    "prisma-client-lib": "^1.34.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "apollo-boost": "^0.4.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "nexus-prisma-generate": "^0.3.7",
    "nodemon": "^1.19.1",
    "prisma": "^1.34.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.2"
  },
  "keywords": []
}
